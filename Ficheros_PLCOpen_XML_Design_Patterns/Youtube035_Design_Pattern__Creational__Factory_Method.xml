          <data name="http://www.3s-software.com/plcopenxml/pou" handleUnknown="implementation">
            <pou name="FB_ShapeCreatorEX" pouType="functionBlock">
              <interface>
                <localVars>
                  <variable name="pCustomShape">
                    <type>
                      <pointer>
                        <baseType>
