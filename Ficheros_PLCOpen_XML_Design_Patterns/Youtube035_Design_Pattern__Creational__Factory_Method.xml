                </addData>
              </interface>
              <body>
                <ST>
                  <xhtml xmlns="http://www.w3.org/1999/xhtml" />
                </ST>
              </body>
              <addData>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="M_GetShapeObject" ObjectId="1a8a6fe2-94ff-4408-9581-9645aebf1956">
                    <interface>
                      <returnType>
                        <derived name="I_shape" />
                      </returnType>
                    </interface>
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF eShape = E_Shape.Custom THEN
	
	//Dynamic instantiation  
	pCustomShape := __NEW(FB_CustomShape);
	iShape := pCustomShape^;
	
	//Return the object
	IF iShape &lt;&gt; 0 THEN
		M_GetShapeObject := iShape;
	END_IF
	
	//Release the memory
	IF (pCustomShape &lt;&gt; 0) THEN
		__DELETE(pCustomShape);
		pCustomShape := 0;
	END_IF
	
	eShape := E_Shape.init;
ELSE
	M_GetShapeObject := SUPER^.M_GetShapeObject();
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD M_GetShapeObject : I_shape
VAR_INPUT
END_VAR
</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
